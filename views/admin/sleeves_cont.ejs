<div class="cat_designs_containers"  id="sleeves_cont" style="display:none">
   

    <div class="show trending_designs_" id="sleeves2_Trending_Designs">
        <!-- <button class="adddesigns" onclick="document.getElementById('trending_design_form').style.display = 'block'">Add Image</button> -->
        <h1>Sleeves Trending Designs</h1>
        <% if(SleevesTrending != null) { %>
            <% SleevesTrending.reverse().forEach(function(item){ %>
                <div  class="trending_designs_child"  >
                    <% if(item.itemimg == null) {%>
                        <img  src="\static\blouseimages\images.jpg" alt="">
                    <% } else {%>
                        <div class="trending_designs_image_box" >
                            <a  onclick="Deletion_confirmation(event,this)" href="/Sleeves/delete/trending/<%= SleevesResult._id %>/<%= item._id %>"><i class="material-icons" >delete</i></a> 
                            <img  src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                        </div>
                    <% } %>
                    <div class="trending_designs_title_box" >
                        <h5 ><%= item.itemtitle %></h5>
                         
                    </div>
                </div>
            <% }) %>
        <% } %>  
        

    </div>
    <div class="hide trending_designs_" id="sleeves2_More_Designs">
        <!-- <button class="adddesigns" onclick="document.getElementById('more_design_form').style.display = 'block'">Add Image</button> -->
        <h1 >Sleeves More Designs</h1>
        <% if(SleevesMore != null) { %>
            <% SleevesMore.reverse().forEach(function(item){ %>
                <div  class="trending_designs_child" >
            
                    <% if(item.itemimg == null) {%>
                        <img  src="\static\blouseimages\images.jpg" alt="">
                    <% } else {%>
                        <div class="trending_designs_image_box" >
                            <a onclick="Deletion_confirmation(event,this)"  href="/Sleeves/delete/more/<%= SleevesResult._id %>/<%= item._id %>"><i class="material-icons">delete</i></a>  
                            <img  src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                        </div>
                    <% } %>
                    <div class="trending_designs_title_box" >
                        <h5><%= item.itemtitle %></h5>
                       
                    </div>
                </div>
            <% }) %>
        <% } %>  
       
    </div>
    
    <div class="hide trending_designs_"  id="sleeves2_Slides_Designs" >
        <!-- <button class="adddesigns" onclick="document.getElementById('slides_design_form').style.display = 'block'">Add Image</button> -->
        <h1>Sleeves Slides</h1>
        <% if(SleevesSlides != null) { %>
            <% SleevesSlides.reverse().forEach(function(item){ %>
                <div class="trending_designs_child">
                 
                    <% if(item.itemimg == null) {%>
                        <img  src="\static\blouseimages\images.jpg" alt="">
                    <% } else {%>
                        <div class="trending_designs_image_box">
                            <a onclick="Deletion_confirmation(event,this)" href="/Sleeves/delete/slides/<%= SleevesResult._id %>/<%= item._id %>"><i class="material-icons">delete</i></a>  
                            <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                        </div>
                    <% } %>
                    
                </div>
            <% }) %>
        <% } %>  


    </div>


</div>
