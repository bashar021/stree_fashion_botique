<div class="cat_designs_containers"  id="lehnga_cont" style="display:none">
  

    <div class="show trending_designs_" id="lehnga2_Trending_Designs">
        <!-- <button class="adddesigns" onclick="document.getElementById('trending_design_form').style.display = 'block'">Add Image</button> -->
        <h1>Lehnga Trending Designs</h1>
        <% if(LehngaTrending != null) { %>
            <% LehngaTrending.reverse().forEach(function(item){ %>
                <div class="trending_designs_child" >
                    <% if(item.itemimg == null) {%>
                        <img  src="\static\blouseimages\images.jpg" alt="">
                    <% } else {%>
                        <div  class="trending_designs_image_box" >
                            <a onclick="Deletion_confirmation(event,this)" href="/Lehnga/delete/trending/<%= LehngaResult._id %>/<%= item._id %>"><i class="material-icons" >delete</i></a>  
                            <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                        </div>
                    <% } %>
                    <div class="trending_designs_title_box" >
                        <h5 ><%= item.itemtitle %></h5>
                       
                    </div>
                </div>
            <% }) %>
        <% } %>  
       

    </div>
    <div class="hide trending_designs_" id="lehnga2_More_Designs">
        <!-- <button class="adddesigns" onclick="document.getElementById('more_design_form').style.display = 'block'">Add Image</button> -->
        <h1>Lehnga More Designs</h1>
            <% if(LehngaMore != null) { %>
                <% LehngaMore.reverse().forEach(function(item){ %>
                    <div  class="trending_designs_child" >
                        <% if(item.itemimg == null) {%>
                            <img  src="\static\blouseimages\images.jpg" alt="">
                        <% } else {%>
                            <div class="trending_designs_image_box" >
                                <a  onclick="Deletion_confirmation(event,this)" href="/Lehnga/delete/More/<%= LehngaResult._id %>/<%= item._id %>"><i class="material-icons">delete</i></a>  
                                <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                            </div>
                        <% } %>
                        <div class="trending_designs_title_box">
                            <h5 ><%= item.itemtitle %></h5>
                        
                        </div>
                    </div>
                <% }) %>
            <% } %>  
    
      

    </div>

    
    <div class="hide trending_designs_"  id="lehnga2_Slides_Designs" >
        <!-- <button class="adddesigns" onclick="document.getElementById('slides_design_form').style.display = 'block'">Add Image</button> -->
        <h1>Lehnga Slides</h1>
        <% if(LehngaSlides != null) { %>
            <% LehngaSlides.reverse().forEach(function(item){ %>
                <div class="trending_designs_child">
                 
                    <% if(item.itemimg == null) {%>
                        <img  src="\static\blouseimages\images.jpg" alt="">
                    <% } else {%>
                        <div class="trending_designs_image_box">
                            <a onclick="Deletion_confirmation(event,this)" href="/Lehnga/delete/slides/<%= LehngaResult._id %>/<%= item._id %>"><i class="material-icons">delete</i></a>  
                            <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                        </div>
                    <% } %>
                    
                </div>
            <% }) %>
        <% } %>  


    </div>


</div>