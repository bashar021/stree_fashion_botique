<div class="cat_designs_containers" id="paplun_cont" style="display:none">


    <div class="show trending_designs_" id="paplun2_Trending_Designs">
        <!-- <button class="adddesigns" onclick="document.getElementById('trending_design_form').style.display = 'block'">Add -->
            Image</button>
        <h1>Paplun Trending Designs</h1>
        <% if(PaplunTrending !=null) { %>
            <% PaplunTrending.reverse().forEach(function(item){ %>
                <div class="trending_designs_child">
                    <% if(item.itemimg==null) {%>
                        <img src="\static\blouseimages\images.jpg" alt="">
                        <% } else {%>
                            <div class="trending_designs_image_box">
                                <a onclick="Deletion_confirmation(event,this)"
                                    href="/Paplun/delete/trending/<%= PaplunResult._id %>/<%= item._id %>"><i
                                        class="material-icons">delete</i></a>
                                <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>"
                                    alt="">
                            </div>
                            <% } %>
                                <div class="trending_designs_title_box">
                                    <h5>
                                        <%= item.itemtitle %>
                                    </h5>

                                </div>
                </div>
                <% }) %>
                    <% } %>




    </div>
    <div class="hide trending_designs_" id="paplun2_More_Designs">
        <!-- <button class="adddesigns" onclick="document.getElementById('more_design_form').style.display = 'block'">Add -->
            Image</button>
        <h1 style="text-align: center;">Paplun More Designs</h1>
        <% if(PaplunMore !=null) { %>
            <% PaplunMore.reverse().forEach(function(item){ %>
                <div class="trending_designs_child">
                    <% if(item.itemimg==null) {%>
                        <img src="\static\blouseimages\images.jpg" alt="">
                        <% } else {%>
                            <div class="trending_designs_image_box">
                                <a onclick="Deletion_confirmation(event,this)" style="float: bottom;"
                                    href="/Paplun/delete/more/<%= PaplunResult._id %>/<%= item._id %>"><i
                                        class="material-icons">delete</i></a>
                                <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>"
                                    alt="">
                            </div>
                            <% } %>
                                <div class="trending_designs_title_box">
                                    <h5>
                                        <%= item.itemtitle %>
                                    </h5>

                                </div>
                </div>
                <% }) %>
                    <% } %>


    </div>

    
    <div class="hide trending_designs_"  id="paplun2_Slides_Designs" >
        <!-- <button class="adddesigns" onclick="document.getElementById('slides_design_form').style.display = 'block'">Add Image</button> -->
        <h1>Peplum Slides</h1>
        <% if(PaplunSlides != null) { %>
            <% PaplunSlides.reverse().forEach(function(item){ %>
                <div class="trending_designs_child">
                 
                    <% if(item.itemimg == null) {%>
                        <img  src="\static\blouseimages\images.jpg" alt="">
                    <% } else {%>
                        <div class="trending_designs_image_box">
                            <a onclick="Deletion_confirmation(event,this)" href="/Paplun/delete/slides/<%= PaplunResult._id %>/<%= item._id %>"><i class="material-icons">delete</i></a>  
                            <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                        </div>
                    <% } %>
                    
                </div>
            <% }) %>
        <% } %>  


    </div>


</div>