<div class="cat_designs_containers"  id="kids_cont" style="display:none">
    <div class="show trending_designs_" id="kids2_Trending_Designs">
      
        
        <h1 >Kids Trending Designs</h1>
            <% if(KidsTrending != null) { %>
                <% KidsTrending.reverse().forEach(function(item){ %>
                    <div class="trending_designs_child"  >
                        
                        <% if(item.itemimg == null) {%>
                            <img  src="\static\blouseimages\images.jpg" alt="">
                        <% } else {%>
                            <div class="trending_designs_image_box">
                                <a onclick="Deletion_confirmation(event,this)"  id="delete_action" href="/Kids/delete/trending/<%= KidsResult._id %>/<%= item._id %>"><i class="material-icons" >delete</i></a>  
                                <img  src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                            </div>
                        <% } %>
                        <div class="trending_designs_title_box" >
                            <h5 ><%= item.itemtitle %></h5> 
                        </div>
                    </div>
                <% }) %>
            <% } %>  

    </div>
    <div class="hide trending_designs_" id="kids2_More_Designs">
       
        <h1>Kids More Designs</h1>
        <% if(KidsMore != null) { %>
            <% KidsMore.reverse().forEach(function(item){ %>
                <div class="trending_designs_child">
                 
                    <% if(item.itemimg == null) {%>
                        <img  src="\static\blouseimages\images.jpg" alt="">
                    <% } else {%>
                        <div class="trending_designs_image_box">
                            <a onclick="Deletion_confirmation(event,this)" href="/Kids/delete/more/<%= KidsResult._id %>/<%= item._id %>"><i class="material-icons">delete</i></a>  
                            <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                        </div>
                    <% } %>
                    <div  class="trending_designs_title_box" >
                        <h5 ><%= item.itemtitle %></h5>
                        
                    </div>
                </div>
            <% }) %>
        <% } %>  
    </div>

    
    <div class="hide trending_designs_"  id="kids2_Slides_Designs" >
       
        <h1>Kids Slides</h1>
        <% if(KidsSlides != null) { %>
            <% KidsSlides.reverse().forEach(function(item){ %>
                <div class="trending_designs_child">
                 
                    <% if(item.itemimg == null) {%>
                        <img  src="\static\blouseimages\images.jpg" alt="">
                    <% } else {%>
                        <div class="trending_designs_image_box">
                            <a onclick="Deletion_confirmation(event,this)" href="/Kids/delete/slides/<%= KidsResult._id %>/<%= item._id %>"><i class="material-icons">delete</i></a>  
                            <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                        </div>
                    <% } %>
                    
                </div>
            <% }) %>
        <% } %>  


    </div>


</div>