<div class="cat_designs_containers" id="bottomdesigns_cont" style="display:none">
    
  

    <div class="show trending_designs_" id="bottomdesigns2_Trending_Designs">
        <!-- <button class="adddesigns" onclick="document.getElementById('trending_design_form').style.display = 'block'">Add Image</button> -->
        <h1>Bottom Trending Designs</h1>
            <% if(BottomdesignsTrending != null) { %>
                <% BottomdesignsTrending.reverse().forEach(function(item){ %>
                    <div class="trending_designs_child" >
                        <% if(item.itemimg == null) {%>
                            <img  src="\static\blouseimages\images.jpg" alt="">
                        <% } else {%>
                            <div class="trending_designs_image_box">
                                <a onclick="Deletion_confirmation(event,this)" href="/Bottomdesigns/delete/trending/<%= BottomdesignsResult._id %>/<%= item._id %>"><i class="material-icons" >delete</i></a>  
                                <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                            </div>
                        <% } %>
                        <div class="trending_designs_title_box" >
                            <h5 ><%= item.itemtitle %></h5>
                         
                        </div>
                    </div>
                <% }) %>
            <% } %>  
     

    </div>

    <div class="hide trending_designs_" id="bottomdesigns2_More_Designs">
        <!-- <button class="adddesigns" onclick="document.getElementById('more_design_form').style.display = 'block'">Add Image</button> -->
       
        <h1>Bottom More Designs</h1>
            <% if(BottomdesignsMore != null) { %>
                <% BottomdesignsMore.reverse().forEach(function(item){ %>
                    <div class="trending_designs_child">
                     
                        <% if(item.itemimg == null) {%>
                            <img  src="\static\blouseimages\images.jpg" alt="">
                        <% } else {%>
                            <div class="trending_designs_image_box" >
                                <a  onclick="Deletion_confirmation(event,this)"  href="/Bottomdesigns/delete/more/<%= BottomdesignsResult._id %>/<%= item._id %>"><i class="material-icons" >delete</i></a>  
                                <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                            </div>
                        <% } %>
                        <div  class="trending_designs_title_box" >
                            <h5><%= item.itemtitle %></h5>
                           
                        </div>
                    </div>
                <% }) %>
            <% } %>  

    </div>


    
    <div class="hide trending_designs_"  id="bottomdesigns2_Slides_Designs" >
        <!-- <button class="adddesigns" onclick="document.getElementById('slides_design_form').style.display = 'block'">Add Image</button> -->
        <h1>Bottom Slides</h1>
        <% if(BottomdesignsSlides != null) { %>
            <% BottomdesignsSlides.reverse().forEach(function(item){ %>
                <div class="trending_designs_child">
                 
                    <% if(item.itemimg == null) {%>
                        <img  src="\static\blouseimages\images.jpg" alt="">
                    <% } else {%>
                        <div class="trending_designs_image_box">
                            <a onclick="Deletion_confirmation(event,this)" href="/Bottomdesigns/delete/slides/<%= BottomdesignsResult._id %>/<%= item._id %>"><i class="material-icons">delete</i></a>  
                            <img src="data:image/<%=item.itemimg.contentType%>;base64,<%=item.itemimg.data.toString('base64')%>" alt="">
                        </div>
                    <% } %>
                    
                </div>
            <% }) %>
        <% } %>  


    </div>


</div>